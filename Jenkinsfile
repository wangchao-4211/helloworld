pipeline {
  agent any
  stages {
    stage('Stage 1') {
      agent any
      steps {
        echo 'Hello world!'
      }
    }
    stage('test') {
      steps {
        writeFile(file: 'redss', text: 'fgfggfv')
      }
    }
    stage('report') {
      steps {
        junit 'testreport'
      }
    }
  }
}